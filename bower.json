{
  "name": "AnimatedHeatMap",
  "version": "1.0.0",
  "authors": [
    "Christopher Cote <chris@entropealabs.com>"
  ],
  "description": "Animated heat map over time using heatmap.js, Leaflet and TimespanEmitter",
  "main": "animatedheatmap.js",
  "moduleType": [
    "amd",
    "globals"
  ],
  "keywords": [
    "heatmap",
    "animated",
    "leaflet",
    "timespan"
  ],
  "license": "MIT",
  "homepage": "github.com/NationalAssociationofREALTORS/AnimatedHeatmap",
  "ignore": [

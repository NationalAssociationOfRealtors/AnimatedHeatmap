    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "TimespanEmitter": "*",
    "jquery": "~2.2.1",
    "leaflet": "~0.7.7",
    "leaflet.heat": "Leaflet.heat#*"
  }
}
